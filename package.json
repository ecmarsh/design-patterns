{
  "name": "design-patterns",
  "version": "0.0.1",
  "description": "Object-oriented design patterns notes and typescript examples.",
  "main": "index.js",
  "scripts": {
    "t": "jest",
    "t:watch": "jest --watch",
    "t:cover": "jest --collect-coverage",
    "t:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "upgrade": "rm -rf node_modules && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ecmarsh/design-patterns.git"
  },
  "keywords": [
    "oop",
    "patterns",
    "object-oriented",
    "gof"
  ],
  "author": {
    "name": "Ethan Marsh",
    "email": "emarsh55@gmail.com"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@types/node": "^12.6.2",
    "@typescript-eslint/eslint-plugin": "^1.12.0",
    "@typescript-eslint/parser": "^1.12.0",
    "eslint": "^5.16.0",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.3"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "env": {
      "node": true
    },
    "rules": {
      "indent": "off",
      "@typescript-eslint/indent": [
        "error",
        2
      ],
      "@typescript-eslint/member-delimiter-style": "off",
      "@typescript-eslint/no-use-before-define": "off",
      "@typescript-eslint/no-parameter-properties": "off",
      "@typescript-eslint/explicit-function-return-type": "warn"
    }
  }
}
